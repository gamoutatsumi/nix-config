{
  "$schema": "https://raw.githubusercontent.com/denoland/deno/refs/heads/main/cli/schemas/config-file.v1.json",
  "tasks": {
    "lint": "deno lint && deno check ./**/*.ts",
    "molt": "deno run --allow-env --allow-read --allow-write --allow-run=git,deno --allow-net jsr:@molt/cli ./**/*.ts --write"
  },
  "lock": false,
  "fmt": {
    "exclude": [
      ".deno",
      ".git"
    ]
  },
  "lint": {
    "exclude": [
      ".deno",
      ".git"
    ]
  },
  "scopes": {},
  "imports": {
    "@shougo/ddu-vim": "jsr:@shougo/ddu-vim@11.1.0",
    "@denops/std": "jsr:@denops/std@8.2.0",
    "@std/path": "jsr:@std/path@1.1.4",
    "@core/unknownutil": "jsr:@core/unknownutil@4.3.0",
    "@kuuote/ddu-kind-git-status": "jsr:@kuuote/ddu-kind-git-status@1.1.0",
    "@shougo/ddc-vim": "jsr:@shougo/ddc-vim@10.1.0",
    "@shougo/dpp-vim": "jsr:@shougo/dpp-vim@6.0.0",
    "@std/fs": "jsr:@std/fs@1.0.22"
  }
}
